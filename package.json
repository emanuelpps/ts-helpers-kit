{
  "name": "ts-helpers-kit",
  "version": "1.0.7",
  "description": "A lightweight utility library for TypeScript projects",
  "keywords": [
    "typescript",
    "helpers",
    "utilities",
    "toolkit",
    "ts",
    "localStorage",
    "cookies",
    "dates",
    "strings",
    "frontend",
    "typescript-library",
    "utility-functions",
    "javascript",
    "react",
    "vue",
    "angular",
    "nodejs",
    "npm",
    "webdev"
  ],
  "type": "module",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/ts-helpers-kit.js",
      "require": "./dist/ts-helpers-kit.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint . --ext .ts",
    "test": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:mutation": "stryker run"
  },
  "coverage": {
    "provider": "v8",
    "reportsDirectory": "./coverage"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/node": "^24.0.10",
    "@vitest/coverage-v8": "^3.1.1",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "husky": "^8.0.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1"
  }
}